super + Escape
	pkill -usr1 -x sxhkd
XF86AudioRaiseVolume
	pulsemixer --change-volume +5
XF86AudioLowerVolume
	pulsemixer --change-volume -5
XF86MonBrightnessUp
	brillo -q -A 5
XF86MonBrightnessDown
	brillo -q -U 5
XF86AudioPlay
	mpc toggle
XF86AudioNext
	mpc next
XF86AudioPrev
	mpc prev
super + Print
	maim | tee ~/pix/ss/$(date +%s).png | xclip -selection clipboard -t image/png
super + x
	slock
